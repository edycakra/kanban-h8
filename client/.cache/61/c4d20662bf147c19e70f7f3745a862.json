ginForm.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":112,"column":0}},{"source":"src/components/LoginForm.vue","name":null,"original":{"line":38,"column":0},"generated":{"line":113,"column":0}}],"sources":{"src/components/LoginForm.vue":"<template> \n        <!-- login  -->\n        <div class=\"container has-text-centered\" v-if=\"currentPage === 'login'\">\n            <div class=\"column is-4 is-offset-4\">\n                <div class=\"box\">\n                    <h3 class=\"title has-text-black\">Login</h3><hr>\n                    <form id=\"login-form\" @submit.prevent=\"loginUser\">\n                        <div class=\"field\">\n                            <label class=\"label\">Email</label>\n                            <div class=\"control has-icons-left has-icons-right\">\n                              <input class=\"input is-danger\" type=\"email\" placeholder=\"type your email here\" id=\"emailLogin\" v-model=\"login.email\" required>\n                              <span class=\"icon is-small is-left\"><i class=\"fas fa-envelope\"></i></span>\n                            </div>\n                        </div>\n                        <div class=\"field\">\n                            <label class=\"label\">Password</label>\n                            <div class=\"control has-icons-left\">\n                              <input class=\"input is-link\" type=\"password\" placeholder=\"type your password here\" id=\"passwordLogin\" v-model=\"login.password\" required>\n                              <span class=\"icon is-small is-left\"><i class=\"fas fa-lock\"></i></span>\n                            </div>\n                        </div>\n                        <button type=\"submit\" class=\"button is-block is-primary is-fullwidth has-text-black\">Login</button>\n                    </form>\n                </div>\n                <p class=\"has-text-grey\">\n                    Not registered yet? Create a new account <a href=\"#\" @click.prevent=\"showRegister\">here</a>\n                </p>\n                or\n                <p><g-signin-button :params=\"googleSignInParams\" @success=\"onSignInSuccess\" @error=\"onSignInError\" >\n                Sign in with Google\n                </g-signin-button></p>\n            </div>\n        </div>\n</template>\n\n<script>\nimport axios from '../config'\nexport default {\n    name: 'LoginForm',\n    data() {\n        return {\n            login: {\n                email: '',\n                password: ''\n            },\n            googleSignInParams: {\n              client_id: '723559606797-1tj1i63gto78likd6lrmc5r6mogrkve9.apps.googleusercontent.com'\n            }                       \n        }\n    },\n    props: ['currentPage'],\n    methods: {\n        loginUser() {\n            axios({\n                method: 'POST',\n                url: `/login`,\n                data: {\n                    email: this.login.email,\n                    password: this.login.password\n                }\n            })\n            .then(({data}) => {\n                localStorage.setItem('access_token', data.access_token)\n                this.$emit('fetchAll')\n                // this.currentPage = 'dashboard'\n                this.$emit('changePage','dashboard')\n            })\n            .catch(response => {\n                console.log(response)\n            })\n        },\n        showRegister() {\n            this.$emit('showRegister')\n        },\n        onSignInSuccess (googleUser) {\n        var id_token = googleUser.getAuthResponse().id_token;\n            axios({\n                method: 'POST',\n                url: `/googleSignIn`,\n                headers: {access_token: id_token}\n            })\n            .then(({data}) => {\n                localStorage.setItem('access_token', data.access_token)\n                this.$emit('fetchAll')\n                // this.currentPage = 'dashboard'\n                this.$emit('changePage','dashboard')\n            })\n            .catch(response => {\n                console.log(response)\n            })\n        },\n        onSignInError (error) {\n            console.log('OH NOES', error)\n        }\n    }\n}\n</script>\n\n<style>\n.g-signin-button {\n  /* This is where you control how the button looks. Be creative! */\n  display: inline-block;\n  padding: 4px 8px;\n  border-radius: 3px;\n  background-color: #3c82f7;\n  color: #fff;\n  box-shadow: 0 3px 0 #0f69ff;\n  cursor: pointer;\n}\n</style>"},"lineCount":null}},"error":null,"hash":"c7dbe214a6f8d5b9f7ef045d0ebcbbfe","cacheData":{"env":{}}}